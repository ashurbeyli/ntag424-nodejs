{
  "name": "ntag424-nodejs",
  "version": "1.0.0",
  "description": "A Node.js library for NTAG424 SDM (Secure Dynamic Messaging) authentication and data verification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "example": "node dist/examples/basic-usage.js",
    "cli": "node dist/cli/index.js"
  },
  "keywords": [
    "ntag424",
    "nfc",
    "sdm",
    "secure-dynamic-messaging",
    "authentication",
    "cmac",
    "aes",
    "rfid",
    "nfc-tag"
  ],
  "author": "Jeyhun Ashurbayov <c.ashurbeyli@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashurbeyli/ntag424-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/ashurbeyli/ntag424-nodejs/issues"
  },
  "homepage": "https://github.com/ashurbeyli/ntag424-nodejs#readme",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {}
}
